<?xml version="1.0" encoding="UTF-8"?>  
<beans  
	xmlns="http://www.springframework.org/schema/beans"  
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
	xmlns:context="http://www.springframework.org/schema/context"  
	xsi:schemaLocation="  
	http://www.springframework.org/schema/beans        
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd  
	http://www.springframework.org/schema/context                
	http://www.springframework.org/schema/context/spring-context-3.0.xsd">  
	
	
	<bean name="paramHello" class="com.czx.bean.ParamHelloService">
		<constructor-arg name="name" value="czx hello"></constructor-arg>
		<constructor-arg index="1" value="25"></constructor-arg>
	</bean>  
	<bean name="paramHello1" class="com.czx.bean.ParamHelloService">
		<constructor-arg name="name" value="czx hello"></constructor-arg>
		<constructor-arg name="age" value="25"></constructor-arg>
	</bean>  
</beans> 
